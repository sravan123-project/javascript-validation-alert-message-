       <script>
        function validateForm() {

            var user = document.getElementById("TextBox1").value;
            var pass = document.getElementById("TextBox2").value;

            if (user == "" && pass == "") {
                alert("Product Code And Name are Mandatory");
                document.getElementById("TextBox1").focus();
                return false;
            }
            else if (user == "") {
                alert("Product Code is  Necessory...");
                document.getElementById("TextBox1").focus();
                return false;
            }
            else if (pass == "") {
                alert("Product Name is Necessory....");
                document.getElementById("TextBox2").focus();
                return false;
            }
            //else if (user.length==0) {
            //    alert("UserName Necessory !Plz Enter Name ....");
            //    document.getElementById("txtUserNama").focus();
            //    return false;
            //}
            //else if (user.length < 4) {
            //    alert("Invalid length username ....");
            //    document.getElementById("txtUserName").focus();
            //    return false;
            //}
            //else if (pass.length < 4 || pass.length >=18) {
            //    alert("Invalid length Password....");
            //    document.getElementById("txtPassword").focus();
            //    return false;
            //}
            return true;
        }
       </script>
